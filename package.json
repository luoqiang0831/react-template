{
  "name": "template",
  "version": "1.0.0",
  "description": "a template",
  "scripts": {
    "start": "cross-env NODE_BUILD=local nodemon --signal SIGHUP --delay 500ms --exec npm run dev",
    "dev": "node server.js --max_old_space_size=4096 --trace-deprecation",
    "build": "cross-env NODE_BUILD=prd webpack --config config/webpack.production.config.js",
    "lint": "prettier --write \"{src,mock}/**/*.{js,ts,tsx,css,less}\"",
    "lint-fix": "eslint . --fix \"{src,mock}/**/*.{js,ts,tsx,jsx,vue}\"",
    "prepare": "husky install",
    "cover": "./node_modules/.bin/istanbul cover _mocha",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "analyz": "cross-env NODE_BUILD=analyz webpack --config config/webpack.production.config.js --progress",
    "clean:lock": "rimraf package-lock.json **/package-lock.json **/**/package-lock.json",
    "clean:modules": "rimraf node_modules **/node_modules **/**/node_modules"
  },
  "author": "luoqiang",
  "private": false,
  "installConfig": {
    "pnp": false
  },
  "dependencies": {
    "@ahooksjs/use-request": "^2.8.15",
    "@loadable/component": "^5.15.2",
    "antd-mobile": "^5.15.1",
    "core-js": "^3.21.1",
    "history": "^5.3.0",
    "i18next": "^21.6.5",
    "js-cookie": "^3.0.1",
    "react": "^18.1.0",
    "react-content-loader": "^6.1.0",
    "react-dom": "^18.1.0",
    "react-i18next": "^11.16.9",
    "react-router-dom": "^6.3.0",
    "umi-request": "^1.4.0",
    "zustand": "^3.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.16.7",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.7",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-arrow-functions": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.18.5",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/runtime": "^7.16.7",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@types/js-cookie": "^3.0.1",
    "@types/loadable__component": "^5.13.4",
    "@types/react": "^18.0.12",
    "@types/react-css-modules": "^4.6.4",
    "@types/react-dom": "^18.0.5",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "autoprefixer": "^10.4.2",
    "axios": "^0.27.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-react-css-modules": "^5.2.6",
    "body-parser": "^1.19.1",
    "copy-webpack-plugin": "^9.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.6.0",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "fork-ts-checker-webpack-plugin": "^6.2.5",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "kill-port": "^1.6.1",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "lint-staged": "^12.3.4",
    "mini-css-extract-plugin": "^2.4.6",
    "mockjs": "^1.1.0",
    "nodemon": "^2.0.15",
    "postcss": "^8.4.7",
    "postcss-less": "^6.0.0",
    "postcss-loader": "^6.2.1",
    "postcss-pxtorem": "^6.0.0",
    "preload-webpack-plugin": "^3.0.0-beta.4",
    "prettier": "^2.5.1",
    "react-css-modules": "^4.7.11",
    "react-refresh": "^0.14.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.0",
    "thread-loader": "^3.0.4",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.2",
    "workbox-webpack-plugin": "^6.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": ["eslint --ext .ts,.tsx,.js --fix", "prettier --write", "git add"],
    "*.@(yml|yaml)": ["prettier --parser yaml --write"],
    "*.md": ["prettier --parser markdown --write"],
    "*.json": ["prettier --parser json --write"]
  }
}
